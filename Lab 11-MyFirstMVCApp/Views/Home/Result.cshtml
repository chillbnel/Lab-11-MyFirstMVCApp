@model List<Lab11MyFirstMVCApp.Models.PersonOfTheYear>
    <head>
        <link herf="StyleSheet.css" />
    </head>
    <body>
        <table>
            <tr>
                <th>Year</th>
                <th>Honor</th>
                <th>Name</th>
                <th>Country</th>
                <th>Birth Year</th>
                <th>Death Year</th>
                <th>Title</th>
                <th>Category</th>
                <th>Context</th>
            </tr>
            @foreach (var person in Model)
            {
                <tr>
                    <td>@person.Year</td>
                    <td>@person.Honor</td>
                    <td>@person.Name</td>
                    <td>@person.Country</td>
                    <td>@person.Birth_Year</td>
                    <td>@person.DeathYear</td>
                    <td>@person.Title</td>
                    <td>@person.Category</td>
                    <td>@person.Context</td>
                </tr>
            }
        </table>
     </body>
